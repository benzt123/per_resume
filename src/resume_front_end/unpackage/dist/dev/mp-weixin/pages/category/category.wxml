<view class="container"><view class="icon-manage-entry" bindtap="{{a}}"><view class="entry-content"><text class="entry-icon">🎨</text><view class="entry-text"><text class="entry-title">图标管理</text><text class="entry-desc">管理分类图标</text></view><text class="entry-arrow">→</text></view></view><view class="type-tabs"><view class="{{['tab-item', b && 'active']}}" bindtap="{{c}}"><text class="tab-text">支出分类</text></view><view class="{{['tab-item', d && 'active']}}" bindtap="{{e}}"><text class="tab-text">收入分类</text></view></view><view class="category-list"><view wx:for="{{f}}" wx:for-item="category" wx:key="d" class="category-item" bindtap="{{category.e}}"><view class="category-info"><text class="category-icon">{{category.a}}</text><text class="category-name">{{category.b}}</text></view><view class="category-usage"><text class="usage-text">已使用{{category.c}}次</text><text class="view-hint">点击查看</text></view></view></view><view class="usage-stats"><text class="stats-title">使用频率统计</text><view wx:if="{{g}}" class="empty-stats"><text class="empty-text">暂无使用记录</text></view><view wx:for="{{h}}" wx:for-item="stat" wx:key="f" class="usage-item"><view class="usage-rank"><text class="rank-number">{{stat.a}}</text></view><view class="usage-info"><text class="usage-icon">{{stat.b}}</text><view class="usage-details"><text class="usage-name">{{stat.c}}</text><text class="usage-count">使用{{stat.d}}次</text></view></view><view class="usage-amount"><text class="amount-text">¥{{stat.e}}</text></view></view></view></view>